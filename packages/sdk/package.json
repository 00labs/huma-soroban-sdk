{
  "name": "@huma-finance/soroban-sdk",
  "version": "0.0.25",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "getting-started.md",
    "API.md",
    "packages"
  ],
  "scripts": {
    "start": "tsc -w",
    "generateDocs": "jsdoc2md --files \"src/**/*.ts\" --configure ./jsdoc2md.json > API.md",
    "build": "tsc --project ./tsconfig.json && tsc --project ./tsconfig.cjs.json",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.{js,ts,tsx,graphql}\"",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,ts,tsx,graphql}\"",
    "test": "jest",
    "test:coverage": "jest --verbose --coverage",
    "test:badges": "yarn run test:coverage && yarn jest-coverage-badges"
  },
  "dependencies": {
    "@huma-finance/soroban-credit-manager": "^0.0.25",
    "@huma-finance/soroban-credit-storage": "^0.0.25",
    "@huma-finance/soroban-huma-config": "^0.0.25",
    "@huma-finance/soroban-pool": "^0.0.25",
    "@huma-finance/soroban-pool-credit": "^0.0.25",
    "@huma-finance/soroban-pool-manager": "^0.0.25",
    "@huma-finance/soroban-pool-storage": "^0.0.25",
    "@huma-finance/soroban-sep41": "^0.0.25",
    "@huma-finance/soroban-tranche-vault": "^0.0.25",
    "@stellar/stellar-sdk": "13.0.0",
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add . && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,graphql}": [
      "yarn generateDocs",
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "yarn test:badges",
      "git add ."
    ],
    "tests/**/*.{js,ts}": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "yarn test:badges",
      "git add ."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.6.1",
    "jsdoc": "^4.0.2",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^8.0.0",
    "lerna": "^8.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.7.1",
    "ts-jest": "^29.1.1"
  }
}
